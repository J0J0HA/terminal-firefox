{
  "manifest_version": 2,
  "name": "> Terminal",
  "version": "1.0",

  "description": "Sets Terminal as default page.",

  "icons": {
    "48": "icons/border-48.png"
  },
  "chrome_url_overrides" : {
    "newtab": "iframed.html"
  },
  "chrome_settings_overrides" : {
    "homepage": "iframed.html"
   },
  "content_scripts": [
    {
        "all_frames": true,
        "matches": ["*://terminal-center.github.io/terminal*"],
        "js": ["injector.js"],
        "run_at": "document_end"
    },
    {
        "matches": ["*://*/*"],
        "js": ["popup.js"],
        "run_at": "document_idle"
    }
  ]
}
